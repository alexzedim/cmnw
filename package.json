{
  "name": "cmnw",
  "version": "6.8.4",
  "homepage": "https://cmnw.me",
  "description": "Intelligence always wins",
  "private": true,
  "license": "MPL 2.0",
  "bugs": {
    "url": "https://github.com/alexzedim/cmnw/issues"
  },
  "author": {
    "name": "alexzedim",
    "url": "https://cmnw.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexzedim/cmnw.git"
  },
  "languageName": "node",
  "packageManager": "pnpm@10.16.0+sha512.8066e7b034217b700a9a4dbb3a005061d641ba130a89915213a10b3ca4919c19c037bec8066afdc559b89635fdb806b16ea673f2468fbb28aabfa13c53e3f769",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/cmnw/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "docker:build": "docker build -f docker/local.Dockerfile -t cmnw:%npm_package_version% -t cmnw:latest .",
    "version": "echo %npm_package_version%"
  },
  "dependencies": {
    "@alexzedim/blizzapi": "2.7.0",
    "@aws-sdk/client-s3": "3.916.0",
    "@bull-board/api": "6.14.0",
    "@bull-board/express": "6.14.0",
    "@bull-board/nestjs": "6.14.0",
    "@nestjs-modules/ioredis": "2.0.2",
    "@nestjs/axios": "4.0.1",
    "@nestjs/bullmq": "11.0.4",
    "@nestjs/common": "11.1.7",
    "@nestjs/core": "11.1.7",
    "@nestjs/mongoose": "11.0.3",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.7",
    "@nestjs/schedule": "6.0.1",
    "@nestjs/swagger": "11.2.1",
    "@nestjs/terminus": "11.0.0",
    "@nestjs/typeorm": "11.0.0",
    "@types/async-csv": "2.1.4",
    "@types/fs-extra": "11.0.4",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "async-csv": "2.1.3",
    "axios": "1.12.2",
    "bullmq": "5.61.2",
    "change-case": "5.4.4",
    "cheerio": "1.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "dotenv": "^17.2.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "express": "5.1.0",
    "farmhash": "5.0.1",
    "fs-extra": "11.3.2",
    "http-proxy-agent": "7.0.2",
    "https-proxy-agent": "7.0.6",
    "ioredis": "5.8.2",
    "lodash": "4.17.21",
    "luxon": "3.7.2",
    "mongoose": "8.19.2",
    "ms": "2.1.3",
    "nestjs-s3": "3.0.1",
    "passport": "0.7.0",
    "passport-discord": "0.1.4",
    "passport-oauth2": "1.8.0",
    "pg": "8.16.3",
    "playwright": "1.56.1",
    "playwright-extra": "4.3.6",
    "prom-client": "^15.1.3",
    "puppeteer-extra-plugin-stealth": "2.11.2",
    "redis": "5.9.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "typeorm": "0.3.27",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.28.5",
    "@jest/globals": "30.2.0",
    "@nestjs/cli": "11.0.10",
    "@nestjs/testing": "11.1.7",
    "@types/config": "3.3.5",
    "@types/express": "5.0.4",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/ms": "2.1.0",
    "@types/node": "24.9.1",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "eslint": "9.38.0",
    "jest": "30.2.0",
    "prettier": "3.6.2",
    "run-script-webpack-plugin": "0.2.3",
    "supertest": "7.1.4",
    "ts-jest": "29.4.5",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "webpack": "5.102.1",
    "webpack-node-externals": "3.0.0"
  },
  "pnpm": {
    "overrides": {
      "@nestjs/terminus": "11.0.0",
      "reflect-metadata": "0.2.2"
    },
    "ignoredBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "farmhash",
      "msgpackr-extract"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(change-case|.*\\.mjs$))"
    ],
    "moduleNameMapper": {
      "@app/configuration": "<rootDir>/libs/configuration/src",
      "@app/configuration/(.*)": "<rootDir>/libs/configuration/src/$1",
      "@app/mongo": "<rootDir>/libs/mongo/src",
      "@app/mongo/(.*)": "<rootDir>/libs/mongo/src/$1",
      "@app/pg": "<rootDir>/libs/pg/src",
      "@app/pg/(.*)": "<rootDir>/libs/pg/src/$1",
      "@app/resources": "<rootDir>/libs/resources/src",
      "@app/resources/(.*)": "<rootDir>/libs/resources/src/$1",
      "^@app/logger(|/.*)$": "<rootDir>/libs/logger/src/$1",
      "^@app/s3(|/.*)$": "<rootDir>/libs/s3/src/$1"
    }
  }
}
